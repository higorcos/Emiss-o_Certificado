<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Meus Eventos</title>
</head>
<body>
    <h3>Meus eventos</h3>
    <table>
        <thead>
        <tr>
            <th>Nome</th>
            <th>Data</th>
            <th>Horas</th>
            <th>Organização</th>
            <th>Certificado</th>
        </tr>
        </thead>
        <tbody>
            <% Subs.forEach(function(sub){ %>
                <%= sub.id %>
              <%= %>
              <% Event.forEach(function(event){ %>
                <% if (event.id == sub.eventId) { %>       
                    <tr> 
                    <th><%= event.name %></th>
                    <th><%= event.date %></th>
                    <th><%= event.hours %></th>
                    <th><%= event.organization %></th>
                    <!-- Fazer teste se o certificado tá liberado para o participante  -->
                    <th>
                    <form id="form_print" action="/user/event/print/<%=sub.id%>" method="POST">
                    <input type='hidden' id="printCertificate" name="printCertificate" value='' />
                    <button>Imprimir certificado </button></form>
                    </th>  
                    </tr>
                                                             <% } %>   
                                                             <%}) %> 
                                                              <%}) %> 
        </tbody>
    </table>
    
    
    <br>
</body>
</html>